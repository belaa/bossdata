from __future__ import division,print_function

import bossdata.path
import bossdata.remote
import bossdata.meta

def main():
    finder = bossdata.path.Finder()
    mirror = bossdata.remote.Manager()
    #path = finder.get_spec_path(6641,56383,11,lite=False)
    #path = finder.get_sp_all_path(lite = False)
    #mirror.get(path)
    meta_db = bossdata.meta.Database(finder,mirror,lite = True)
    #for row in meta_db.select_each(what = '*',where = '`OBJTYPE`="GALAXY"'):
    #    print(row)
    rows = meta_db.select_all(what = '*',where = '`OBJTYPE`="GALAXY"')
    print(rows)

if __name__ == '__main__':
    main()
