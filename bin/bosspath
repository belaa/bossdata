#!/usr/bin/env python

from __future__ import division,print_function

import bossdata.path
import bossdata.remote

def main():
    finder = bossdata.path.Finder()
    #path = finder.get_spec_path(6641,56383,11,lite=False)
    path = finder.get_sp_all_path()
    mgr = bossdata.remote.Manager()
    local_path = mgr.get(path)
    print('File available at {}'.format(local_path))

if __name__ == '__main__':
    main()
